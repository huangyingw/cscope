[remote "code"]
    url = git://git.code.sf.net/p/cscope/cscope
    fetch = +refs/heads/*:refs/remotes/code/*
[branch "dev"]
    remote = origin
    merge = refs/heads/dev
[branch "master"]
    remote = origin
    merge = refs/heads/master
[gsync]
    remote = code
    target = code/master
[push]
    remote = origin
[vrun]
    silent = false
[gdif]
    ignore = ./*.sh
    ignore = ./INSTALL
    ignore = ./INSTALL.gnu
    ignore = ./compile
    ignore = ./config.guess
    ignore = ./config.h.in
    ignore = ./config.sub
    ignore = ./depcomp
    ignore = ./dev.gdio.diff
    ignore = ./install-sh
    ignore = ./missing
    ignore = ./mkinstalldirs
    ignore = ./ylwrap
