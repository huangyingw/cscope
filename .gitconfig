[merge]
    allow = true
[commit]
    message = n
[remote "code"]
    url = git://git.code.sf.net/p/cscope/cscope
    fetch = +refs/heads/*:refs/remotes/code/*
[branch "dev"]
    remote = origin
    merge = refs/heads/dev
[branch "master"]
    remote = origin
    merge = refs/heads/master
[gsync]
    remote = code
    branch = master
[push]
    remote = origin
[vrun]
    silent = false
